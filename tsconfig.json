{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "assets/*" :["./src/assets"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}